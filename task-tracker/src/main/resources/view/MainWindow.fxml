<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox onMouseClicked="#setInitialInputFocus" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ui.MainWindow">
   <children>
      <SplitPane fx:id="splitpane" dividerPositions="0.7">
         <items>
            <VBox fx:id="taskList">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="taskListPanelPlaceholder" />
               </children>
            </VBox>
            <AnchorPane fx:id="listStatisticsPlaceholder" prefWidth="300.0" />
         </items>
      </SplitPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="150.0" prefHeight="100" styleClass="anchor-pane-with-border">
           <padding>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
           </padding>
       </AnchorPane>
      <AnchorPane fx:id="commandBoxPlaceholder" maxHeight="50.0" prefHeight="32.0" prefWidth="690.0" styleClass="anchor-pane-with-border">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </AnchorPane>
      <AnchorPane fx:id="statusbarPlaceholder" nodeOrientation="RIGHT_TO_LEFT" snapToPixel="false">
         <stylesheets>
            <URL value="@DarkTheme.css" />
            <URL value="@Extensions.css" />
         </stylesheets>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
</VBox>
